.b_btn {
  position: fixed;
  bottom: 60rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 634rpx;
  display: flex;
  justify-content: space-around;
  z-index: 1;
}
.b_btn .btn {
  width: 300rpx;
  height: 82rpx;
  line-height: 82rpx;
  border-radius: 41rpx;
  font-size: 36rpx;
  color: #730c15;
  background-color: transparent;
  position: relative;
}
.b_btn .btn .img {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.b_btn .btn::after {
  border: none;
}