/* pages/dl/dl.wxss */
.dl_box {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.dl_box .img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.dl_box .dl_cont {
  width: 658rpx;
  height: 700rpx;
  margin: 108rpx auto 0;
  position: relative;
  z-index: 1;
}
.dl_box .dl_cont .dl {
  display: block;
  width: 100%;
  height: 100%;
}

.b_cont {
  display: flex;
  height: 100rpx;
  width: 580rpx;
  margin: 30rpx auto 0;
  align-items: center;
